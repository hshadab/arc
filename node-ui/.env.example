# Arc Network Configuration
# Copy this file to .env and fill in your actual values

# Private key for your wallet (WITHOUT 0x prefix)
# WARNING: Keep your actual .env file secure and never commit it!
PRIVATE_KEY=your_private_key_here_without_0x

# Arc Testnet RPC
ARC_RPC_URL=https://rpc.testnet.arc.network

# Arc Testnet Chain ID
ARC_CHAIN_ID=5042002

# Deployed contract addresses on Arc Testnet
COMMITMENT_REGISTRY_ADDRESS=0x8d65d93022EB39c1b66c72A7F55C63c0C28B4E12
SPEND_GATE_ADDRESS=0x3D7Ce2Cc674149d8448B1014f34fc2B3b76e18E7

# JOLT-Atlas zkML Configuration (Real proofs enabled by default)
# Real ML inference proofs take ~2-3 minutes per proof
# Comment out these variables to use simulated proofs for faster demo (~5s)
JOLT_PROVER_BIN=/home/hshadab/arc/ooak/node-ui/proof_json_output
JOLT_MODEL_PATH=/home/hshadab/arc/ooak/node-ui/models/simple_text_classification.onnx
OOAK_ONNX_MODEL=/home/hshadab/arc/ooak/node-ui/models/simple_text_classification.onnx

